
<expression> :=		<whitespace> [ <not> ] <letter> <whitespace> <operator> <whitespace>
					( [ <not> ] <letter> | <<expression> ) <whitespace> <implies>
					<whitespace> [ <not> ] <letter> <whitespace> <operator> <whitespace>
					( [ <not> ] <letter> | <<expression> )
<implie> :=			"=>" | "<=>"
<operator> :=		'|' | '&' | '^'
<not> :=			'!'
<letter> :=			'A'..'Z'
<whitespace> :=		{ ' ' | 9..13 }
